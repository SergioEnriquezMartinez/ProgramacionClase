package Empresa;

public class Empleados {
	protected String nombre;
	protected String dni;
	protected int edad;
	protected int sueldo;
	
	public Empleados(String nombre, String dni, int edad, int sueldo) {
		this.nombre = nombre;
		this.dni = dni;
		this.edad = edad;
		this.sueldo = sueldo;
	}
	
	
}
