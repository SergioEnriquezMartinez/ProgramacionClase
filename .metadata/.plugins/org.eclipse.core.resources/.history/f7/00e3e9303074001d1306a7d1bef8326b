package Empresa;

public class Init {
	public static void main(String[] args) {
		
		
		Empleados e1 = new Directivo("Fran", "123456789A", 30, 30000, 0.1);
		Empleados d1 = new Desarrollador("Juan", "987654321B", 24, 19000, true, true, false);
		Empleados t1 = new Tester("Sara", "2342356C", 25, 17000, 4);
		
		System.out.println(e1);
		System.out.println(d1);
		System.out.println(t1);
		
		
	}
	
}
